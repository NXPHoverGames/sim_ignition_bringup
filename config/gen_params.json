			"name": "debug_image_tf_lite_node_real",
			"output": "screen",
			"parameters": [{"debug_topic": "/DebugImageReal"},
							{"tflite_topic": "/TFLiteReal"},
							{"image_topic": "/NPU/image_real"},
							{"threshold": 0.6},
							{"max_returns": 5},
							{"max_queue": 30}]
		},
		"ros2Node_5":{
			"timing": "post-spawn",
			"package": "debug_image_tf_lite",
			"executable": "debug_image_tf_lite_node",
			"name": "debug_image_tf_lite_node_real",
			"output": "screen",
			"parameters": [{"debug_topic": "/DebugImageSim"},
							{"tflite_topic": "/TFLiteSim"},
							{"image_topic": "/NPU/image_sim"},
							{"threshold": 0.55},
							{"max_returns": 5},
							{"max_queue": 30}]
		},
