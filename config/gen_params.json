{	"setup":{
		"fuel": {
			"fuelModels": {
				"fuelModelsRepo_0":{
					"repo": "git@github.com:rudislabs/nxp_fuel.git",
					"version": "EindhovenElMandaderoDemo",
					"name": "nxp_fuel"
				}
			}
		},
		"ros2": {
		}
	},
	"nodes": {
		"ros2Node_0":{
			"package": "ros_ign_bridge",
			"executable": "parameter_bridge",
			"name": "parameter_bridge",
			"output": "screen",
			"arguments": ["/cmd_vel@geometry_msgs/msg/Twist@ignition.msgs.Twist"],
			"parameters": []
		},
		"ros2Node_1":{
			"package": "joy",
			"executable": "joy_node",
			"name": "joy_node",
			"output": "screen",
			"arguments": [],
			"parameters": [{"dev": "/dev/input/js0"},
							{"deadzone": 0.2},
							{"autorepeat_rate": 20.0}]
		},
		"ros2Node_2":{
			"package": "teleop_twist_joy",
			"executable": "teleop_node",
			"name": "teleop_twist_joy_node",
			"output": "screen",
			"arguments": [],
			"parameters": [{"axis_linear.x": 2},
							{"scale_linear.x": -5.0},
							{"scale_linear_turbo.x": -10.0},
							{"axis_angular.yaw": 0},
							{"scale_angular.yaw": 10.0},
							{"scale_angular_turbo.yaw": 20.0},
							{"enable_button": 5},
							{"enable_turbo_button": 6},
							{"require_enable_button": false}],
			"remappings": "('/cmd_vel', 'cmd_vel')"
		}
	},
	"world": {
		"fuelName": "nxp_fuel",
		"name": "ground_plane",
		"params":{
			"skybox": 0,
			"wind": "NotSet",
			"WGS84": {
				"degLatitude": 39.8039,
				"degLongitude": -84.0606,
				"mAltitude": 244,
				"useSphericalCoords": 1
			},
			"sun": {
				"model": "sunUTC",
				"dateUTC": "1904/09/20/17:30"
			},
			"embeddedModels": {
				"embedModel_0": {
					"model": "Raceway",
					"name": "Raceway",
					"pose": "0 0 0 0 0 -1.57"
				},
				"embedModel_2": {
					"model": "El_Mandadero",
					"name": "El_Mandadero",
					"pose": "0 0 .5 0 0 0"
				}
			}
		}
	}
}
