							{"scale_linear_turbo.x": -10.0},
							{"axis_angular.yaw": 0},
							{"scale_angular.yaw": -3.0},
							{"scale_angular_turbo.yaw": -6.0},
							{"enable_button": 5},
							{"enable_turbo_button": 6},
							{"require_enable_button": false}],
			"remappings": "('/cmd_vel', '/requested_vel')"
		},
		"ros2Node_3":{
			"timing": "post-spawn",
			"package": "ros_ign_image",
			"executable": "image_bridge",
